class Solution:
    def fib(self, n: int) -> int:
        if n==0:
            return 0
        if n==1:
            return 1
     
        else:
            return self.fib(n-1)+ self.fib(n-2) #called with self because fib is a method of a class now
                                                #!!method is a function defined in a class
    
solution=Solution()
print(solution.fib(5))
